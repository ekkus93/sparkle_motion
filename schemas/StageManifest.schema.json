{
  "$id": "sparkle_motion/StageManifest.schema.json",
  "description": "Structured artifact manifest entry served via production_agent /artifacts.",
  "properties": {
    "artifact_type": {
      "title": "Artifact Type",
      "type": "string"
    },
    "artifact_uri": {
      "title": "Artifact Uri",
      "type": "string"
    },
    "checksum_sha256": {
      "anyOf": [
        {
          "pattern": "^[0-9a-f]{64}$",
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Checksum Sha256"
    },
    "created_at": {
      "title": "Created At",
      "type": "string"
    },
    "download_url": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Download Url"
    },
    "duration_s": {
      "anyOf": [
        {
          "minimum": 0,
          "type": "number"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Duration S"
    },
    "frame_rate": {
      "anyOf": [
        {
          "minimum": 0,
          "type": "number"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Frame Rate"
    },
    "local_path": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Local Path"
    },
    "media_type": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Media Type"
    },
    "metadata": {
      "additionalProperties": true,
      "title": "Metadata",
      "type": "object"
    },
    "mime_type": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Mime Type"
    },
    "name": {
      "title": "Name",
      "type": "string"
    },
    "notes": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Notes"
    },
    "playback_ready": {
      "anyOf": [
        {
          "type": "boolean"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Playback Ready"
    },
    "qa_mode": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Qa Mode"
    },
    "qa_passed": {
      "anyOf": [
        {
          "type": "boolean"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Qa Passed"
    },
    "qa_report_uri": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Qa Report Uri"
    },
    "qa_skipped": {
      "anyOf": [
        {
          "type": "boolean"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Qa Skipped"
    },
    "resolution_px": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Resolution Px"
    },
    "run_id": {
      "title": "Run Id",
      "type": "string"
    },
    "size_bytes": {
      "anyOf": [
        {
          "minimum": 0,
          "type": "integer"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Size Bytes"
    },
    "stage_id": {
      "title": "Stage Id",
      "type": "string"
    },
    "storage_hint": {
      "anyOf": [
        {
          "enum": [
            "adk",
            "local"
          ],
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Storage Hint"
    }
  },
  "required": [
    "run_id",
    "stage_id",
    "artifact_type",
    "name",
    "artifact_uri"
  ],
  "title": "StageManifest",
  "type": "object"
}